{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "sucrase ./src -d ./build --transforms javascript,imports ",
    "start": "node build/server.js",
    "start:queue": "node build/queue.js",
    "dev": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "run:migrations": "npx sequelize db:migrate",
    "postgis:creator": "docker run --name gobarber-postgis -p 5432:5432 -d -t kartoza/postgis",
    "mongo:creator": "docker run --name gobarber-mongo -d -p 27017:27017/tcp mongo:latest",
    "redis:creator": "docker run --name gobarber-redis -d -p 6379:6379/tcp redis:latest"
  },
  "dependencies": {
    "@sentry/node": "5.4.3",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.0.0-beta.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "nodemailer-express-handlebars": "^3.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.15.1",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "sucrase": "^3.10.1"
  }
}
